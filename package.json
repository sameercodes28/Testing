{
  "name": "karin-gunnerek-portfolio",
  "version": "2.1.0",
  "description": "Architect portfolio website for Karin Gunnerek - Bilingual (SV/EN) with JSON-driven content",
  "author": "Karin Gunnerek",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://karingunnerek.se",
  "repository": {
    "type": "git",
